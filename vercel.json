{
    "version": 2,
    "functions": {
      "api/index.php": {
        "runtime": "vercel-php@0.7.4"
      }
    },
    "routes": [
      {
        "src": "/(css|js|images|fonts|build)/(.*)",
        "dest": "public/$1/$2"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "false",
      "LOG_CHANNEL": "stderr",
      "SESSION_DRIVER": "cookie"
    },
    "buildCommand": "npm install && npm run build",
    "outputDirectory": "public/build"
  }
  